<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>

			<li Class="CombatExtended.PatchOperationFindMod">
				<modName>[TMC].30 Carbines</modName>
			</li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="TMC_IAR_MOneCarbine"]/verbs/li/verbClass</xpath>
                <value><verbClass>Jams_nmags.Verb_ShootWithReliability</verbClass></value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="TMC_IAR_MOneCarbine"]/comps</xpath>
                <value>
                <li Class="Jams_nmags.ReliabilityCompProps">
                    <Reliability>9500</Reliability>
                </li>
                </value>
            </li>


             <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="TMC_IAR_MOneA1Carbine"]/verbs/li/verbClass</xpath>
                <value><verbClass>Jams_nmags.Verb_ShootWithReliability</verbClass></value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="TMC_IAR_MOneA1Carbine"]/comps</xpath>
                <value>
                <li Class="Jams_nmags.ReliabilityCompProps">
                    <Reliability>9500</Reliability>
                </li>
                </value>
            </li>




             <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="TMC_IAR_MTwoCarbine"]/verbs/li/verbClass</xpath>
                <value><verbClass>Jams_nmags.Verb_ShootWithReliability</verbClass></value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="TMC_IAR_MTwoCarbine"]/comps</xpath>
                <value>
                <li Class="Jams_nmags.ReliabilityCompProps">
                    <Reliability>9200</Reliability>
                </li>
                </value>
            </li>



         


            
             



            
            
        </operations>
</Operation>
</Patch>